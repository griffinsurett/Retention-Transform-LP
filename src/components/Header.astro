---
import { SiteData } from '@/content/SiteData';
import { Picture } from 'astro:assets';
---
<header class="header w-full min-h-1/3 flex justify-center items-center"> 
  <a href="/">
    <Picture 
      src={SiteData.logo.src} 
      alt={SiteData.title} 
      formats={['avif', 'webp']} 
      fallbackFormat="png" 
      loading="eager" 
      fetchpriority="high" 
      width={520} 
      height={210} 
    />
  </a>
</header>
